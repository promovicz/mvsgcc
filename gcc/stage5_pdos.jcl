//GCCGEN   JOB CLASS=C,REGION=0K,TIME=1440
//*
//*
//UNLOAD   PROC PDPPREF='PDPCLIB',MEMBER=''
//*
//* Do the IEBCOPY unload
//*
//IEBCOPY  EXEC PGM=IEBCOPY
//SYSUT1   DD DSN=&PDPPREF..LINKLIB,DISP=SHR
//SYSUT2   DD DSN=&&COPY,SPACE=(CYL,(1,1)),UNIT=SYSALLDA,
//         DISP=(NEW,PASS)
//SYSPRINT DD SYSOUT=*
//* This needs an override
//SYSIN    DD DUMMY
//*
//* Now convert to RDW format, which creates an MVS PE executable
//*
//COPYFILE EXEC PGM=COPYFILE,PARM='-bb dd:in dd:out'
//STEPLIB  DD  DSN=&PDPPREF..LINKLIB,DISP=SHR
//IN       DD  DSN=&&COPY,DISP=(OLD,DELETE)
//OUT      DD  DSN=&&MVSPE(&MEMBER),DISP=(OLD,PASS)
//SYSPRINT DD  SYSOUT=*
//SYSTERM  DD  SYSOUT=*
//SYSIN    DD  DUMMY
//*
//         PEND
//*
//*
//*
//CREATE   EXEC PGM=IEFBR14
//DD0      DD DSN=&&MVSPE,DISP=(,PASS),
// DCB=(RECFM=U,LRECL=0,BLKSIZE=6233),
// SPACE=(CYL,(10,10,44)),UNIT=SYSALLDA
//*
//*
//S0 EXEC UNLOAD,MEMBER=GCC
//IEBCOPY.SYSUT1 DD DSN=GCC.LINKLIB,DISP=SHR
//IEBCOPY.SYSIN DD *
 COPY INDD=((SYSUT1,R)),OUTDD=SYSUT2
 SELECT MEMBER=GCC
/*
//*
//*
//S1 EXEC UNLOAD,MEMBER=HEXDUMP
//IEBCOPY.SYSIN DD *
 COPY INDD=((SYSUT1,R)),OUTDD=SYSUT2
 SELECT MEMBER=HEXDUMP
/*
//*
//*
//S2 EXEC UNLOAD,MEMBER=MVSUNZIP
//IEBCOPY.SYSIN DD *
 COPY INDD=((SYSUT1,R)),OUTDD=SYSUT2
 SELECT MEMBER=MVSUNZIP
/*
//*
//*
//S3 EXEC UNLOAD,MEMBER=COPYFILE
//IEBCOPY.SYSIN DD *
 COPY INDD=((SYSUT1,R)),OUTDD=SYSUT2
 SELECT MEMBER=COPYFILE
/*
//*
//*
//S4 EXEC UNLOAD,MEMBER=MVSENDEC
//IEBCOPY.SYSIN DD *
 COPY INDD=((SYSUT1,R)),OUTDD=SYSUT2
 SELECT MEMBER=MVSENDEC
